@model IEnumerable<MVC_MyMusicStore.Models.Genre>
@{
    ViewBag.Title = "Index";
}
<h2>This is the Home Page</h2>
<ul>
    @foreach (var genre in Model)
    {
            <li>
            @Html.ActionLink(genre.Name,
                              "Browse", new { genre = genre.Name })
            </li>
    }
</ul>